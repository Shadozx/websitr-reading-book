<!DOCTYPE html>
<html lang="en">
  </style>
  <head>
    <%- include('./head')%>
    <link href="public/navbar-static-top.css" rel="stylesheet">

    <style>
      h1{
        font-size: small;
      }
    body {
      min-height: 2000px;
    }
    
    .navbar-static-top {
      margin-bottom: 19px;
    }
    </style>
  </head>
  <body>
    <%- include('./header')%>
    
    <div class="container">

      <!-- Main component for a primary marketing message or call to action -->
      <div class="jumbotron">
        <h1><%=title%></h1>
        <%page.textPage.forEach(function(p) { %>
          <p><%=p%></p>
          <%})%>
      </div>
      <nav aria-label="...">
        <ul class="pager">
          <% if(page.next && page.previously) {%>
          <li class="previous"><a href="<%=page.previously%>"><span aria-hidden="true">&larr;</span> Previously page</a></li>
          <li class="next"><a href="<%=page.next%>">Next page<span aria-hidden="true">&rarr;</span></a></li>
          <%} else {%>
            <% if(page.next) {%>
              <li class="next"><a href="<%=page.next%>">Next page<span aria-hidden="true">&rarr;</span></a></li>
            <%}%>
            <% if(page.previously) {%>
              <li class="previous"><a href="<%=page.previously%>"><span aria-hidden="true">&larr;</span> Previously page</a></li>
            <%}%>
          <%}%> 
        </ul>
      </nav>
    </div>
  </body>
</html>
